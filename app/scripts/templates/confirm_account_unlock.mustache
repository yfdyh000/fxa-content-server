<div id="main-content">
  <header>
    <h1 id="fxa-confirm-account-unlock-header">{{#t}}Unlock email sent{{/t}}</h1>
  </header>

  <section>
    <div class="error"></div>
    <div class="success">
      {{#t}}Email resent{{/t}}
    </div>

    <div class="graphic graphic-mail">{{#t}}Email Sent{{/t}}</div>

    <p class="verification-email-message">{{#t}}For security reasons your account was put on lockdown. To perform account-level activities, please re-verify your account by clicking the verification link sent to %(email)s{{/t}}</p>

    <div class="links">
      <a id="back" class="left" href="#">{{#t}}Back{{/t}}</a>
      <a id="resend" class="right" href="#">{{#t}}Resend email{{/t}}</a>
    </div>

  </section>
</div>
